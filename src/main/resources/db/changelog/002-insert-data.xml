<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.10.xsd">

    <changeSet id="002-insert-user-data" author="yaromich">
        <insert tableName="users">
            <column name="id" valueSequenceNext="user_seq"/>
            <column name="login" value="Anastasia"/>
            <column name="password" value="12345"/>
            <column name="balance" value="0.0"/>
        </insert>
    </changeSet>

    <changeSet id="002-insert-role1-data" author="yaromich">
        <insert tableName="roles">
            <column name="id" valueSequenceNext="role_seq"/>
            <column name="name" value="ROLE_USER"/>
        </insert>
    </changeSet>

    <changeSet id="002-insert-role2-data" author="yaromich">
        <insert tableName="roles">
            <column name="id" valueSequenceNext="role_seq"/>
            <column name="name" value="ROLE_ADMIN"/>
        </insert>
    </changeSet>
</databaseChangeLog>